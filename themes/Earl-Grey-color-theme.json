        "tokenColors": [
                {
                        "name": "Comments",
                        "scope": [
                                "comment",
                                "punctuation.definition.comment"
                        ],
                        "settings": {
                                "fontStyle": "italic",
                                "foreground": "#9E9A95"
                        }
                },
                {
                        "name": "Comments: Preprocessor",
